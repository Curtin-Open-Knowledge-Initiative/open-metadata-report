{
  "cache_bucket_name": "output_cache",
  "output_bucket_name": "output_files",
  "logfile": "logs/precipy.log",
  "loglevel": "DEBUG",
  "constants": {
    "PROJECT_ID": "utrecht-university",
    "YEAR_RANGE": [
      1980,
      2026
    ]
  },
  "analytics": [
    [
      "source_to_intermediate",
      {}
    ],
    [
      "intermediate_to_source_truthtable",
      {
        "depends": [
          "source_to_intermediate"
        ]
      }
    ],
    [
      "openalex_native_to_truthtable",
      {
        "depends": []
      }
    ],
    [
      "crossref_member_status",
      {
      }
    ],
    [
      "crossref_to_truthtable",
      {
        "depends": [
          "crossref_member_status"
        ],
        "source": "crossref"
      }
    ],
    [
      "source_category_query",
      {
        "depends": [
          "intermediate_to_source_truthtable"
        ],
        "source": "mag"
      }
    ],
    [
      "source_category_query",
      {
        "depends": [
          "intermediate_to_source_truthtable"
        ],
        "source": "openalex"
      }
    ],
    [
      "source_category_query",
      {
        "depends": [
          "openalex_native_to_truthtable"
        ],
        "source": "openalex_native"
      }
    ],
    [
      "dois_category_query",
      {
        "depends": [
          "crossref_to_truthtable",
          "intermediate_to_source_truthtable",
          "openalex_native_to_truthtable"
        ],
        "rerun": true
      }
    ],
    [
      "graphs.value_add_graphs",
      {
        "depends": [
          "dois_category_query"
        ]
      }
    ],
    [
      "graphs.overall_comparison",
      {
        "depends": [
          "dois_category_query",
          "source_category_query"
        ]
      }
    ],
    [
      "graphs.source_coverage_by_crossref_type",
      {
        "depends": [
          "dois_category_query"
        ]
      }
    ],
    [
      "graphs.source_in_base_by_pubdate",
      {
        "depends": [
          "dois_category_query",
          "source_category_query"
        ]
      }
    ],
    [
      "git_status",
      {
        "depends": [
          "source_to_intermediate",
          "intermediate_to_source_truthtable",
          "crossref_member_status",
          "crossref_to_truthtable",
          "source_category_query",
          "dois_category_query",
          "graphs.value_add_graphs"
        ]
      }
    ]
  ],
  "templates": [
    "report_template.md"
  ],
  "filters": [
    [
      "markdown",
      "html"
    ],
    [
      "xhtml2pdf",
      "pdf"
    ]
  ]
}