{
  "cache_bucket_name": "output_cache",
  "output_bucket_name": "output_files",
  "logfile": "logs/precipy.log",
  "loglevel": "DEBUG",
  "constants": {
    "PROJECT_ID": "utrecht-university",
    "YEAR_RANGE": [
      1980,
      2026
    ]
  },
  "analytics": [
    [
      "source_to_intermediate",
      {
        "source": "mag"
      }
    ],
    [
      "source_to_intermediate",
      {
        "source": "openalex"
      }
    ],
    [
      "create_qdoi_table",
      {
        "depends": [
          "source_to_intermediate"
        ]
      }
    ],
    [
      "query_intermediates_categories",
      {
        "depends": [
          "source_to_intermediate"
        ],
        "source": "mag"
      }
    ],
    [
      "query_intermediates_categories",
      {
        "depends": [
          "source_to_intermediate"
        ],
        "source": "openalex"
      }
    ],
    [
      "crossref_member_status",
      {
      }
    ],
    [
      "query_qdoi_categories",
      {
        "depends": [
          "create_qdoi_table",
          "crossref_member_status"
        ]
      }
    ],
    [
      "git_status",
      {
        "depends": [
          "source_to_intermediate",
          "create_qdoi_table",
          "query_intermediates_categories",
          "crossref_member_status",
          "query_qdoi_categories"
        ]
      }
    ]
  ],
  "templates": [
    "report_template.md"
  ],
  "filters": [
    [
      "markdown",
      "html"
    ],
    [
      "xhtml2pdf",
      "pdf"
    ]
  ]
}