{
  "cache_bucket_name": "output_cache",
  "output_bucket_name": "output_files",
  "logfile": "logs/precipy.log",
  "loglevel": "DEBUG",
  "constants": {
    "PROJECT_ID": "utrecht-university",
    "YEAR_RANGE": [
      1980,
      2023
    ]
  },
  "analytics": [
    [
      "openalex_to_truthtable",
      {
        "depends": [],
        "rerun": false
      }
    ],
    [
      "crossref_to_truthtable",
      {
        "source": "crossref",
        "rerun": false
      }
    ],
    [
      "source_category_query",
      {
        "depends": [
          "openalex_to_truthtable"
        ],
        "rerun": true
      }
    ],
    [
      "dois_category_query",
      {
        "depends": [
          "crossref_to_truthtable",
          "openalex_to_truthtable"
        ],
        "rerun": true
      }
    ],
    [
      "value_add_graphs",
      {
        "depends": [
          "dois_category_query"
        ]
      }
    ],
    [
      "value_add_self_graphs",
      {
        "depends": [
          "source_category_query"
        ]
      }
    ],
    [
      "overall_comparison",
      {
        "depends": [
          "dois_category_query",
          "source_category_query"
        ]
      }
    ],
    [
      "source_coverage_by_crossref_type",
      {
        "depends": [
          "dois_category_query"
        ]
      }
    ],
    [
      "source_coverage_self_by_type",
      {
        "depends": [
          "source_category_query"
        ]
      }
    ],
    [
      "source_in_base_by_pubdate",
      {
        "depends": [
          "dois_category_query",
          "source_category_query"
        ]
      }
    ],
    [
      "dois_in_source_by_pubdate",
      {
        "depends": [
          "dois_category_query",
          "source_category_query"
        ]
      }
    ],
    [
      "generate_tables",
      {
        "depends": [
          "dois_category_query",
          "source_category_query"
        ]
      }
    ],
    [
      "save_data_parameters",
      {
        "depends": [
          "crossref_to_truthtable",
          "source_category_query",
          "dois_category_query",
          "value_add_graphs",
          "overall_comparison",
          "source_coverage_by_crossref_type",
          "source_in_base_by_pubdate"
        ]
      }
    ],
    [
      "git_status",
      {
        "depends": [
          "crossref_to_truthtable",
          "source_category_query",
          "dois_category_query",
          "value_add_graphs",
          "overall_comparison",
          "source_coverage_by_crossref_type",
          "source_in_base_by_pubdate"
        ]
      }
    ]
  ],
  "templates": [
    "report_template_singleDB.md"
  ],
  "filters": [
    [
      "markdown",
      "html"
    ],
    [
      "xhtml2pdf",
      "pdf"
    ]
  ]
}